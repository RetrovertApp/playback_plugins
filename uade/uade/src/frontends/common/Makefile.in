AR = {AR}
CC = {CC}
CFLAGS = -Wall -O2 -I../include -I../common {DEBUGFLAGS} {ARCHFLAGS} {BENCODETOOLSFLAGS} {ZAKALWEFLAGS}
CLIBS = {ARCHLIBS} -lm {BENCODETOOLSLIBS} {ZAKALWELIBS}

all:	libuade.a

%.o:	%.c
	$(CC) $(CFLAGS) -c $<

include ../common/Makefile.common

libuade.a: $(COMMONMODULES)
	$(AR) rcs $@ $(COMMONMODULES)

clean:	
	rm -f *.o *.a

check:	all

installcheck:	check

distclean:	clean
